body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

#RND
  width 1600px
  font-size 8px
  line-height 8px

#RND .tile
  display block
  width 30px
  height 30px
  float left
  border 1px solid #aaa

#RND .tile.ground
  background #663333


#RND .tile.wall
  background red

#RND .tile.close
  background gray

#RND .tile.open
  background greenyellow

#RND .tile.path
  background blue